{
  "shell.nix": {
    "prefix": ["shell"],
    "body": [
      "{ pkgs ? import <nixpkgs> {} }:",
      "pkgs.mkShell {",
      "  packages = [];",
      "  buildInputs = [];",
      "}"
    ]
  },
  "flake-shell": {
    "prefix": ["shell.flake"],
    "body": [
      "{",
      "  inputs = {",
      "    nixpkgs.url = \"github:nixos/nixpkgs/nixpkgs-unstable\";",
      "    flake-utils.url = \"github:numtide/flake-utils\";",
      "  };",
      "  outputs = {",
      "    nixpkgs,",
      "    flake-utils,",
      "    ...",
      "  }:",
      "    flake-utils.lib.eachDefaultSystem (system:",
      "      let",
      "        pkgs = import nixpkgs { inherit system; };",
      "      in {",
      "        devShell = pkgs.mkShell rec {",
      "          packages = [];",
      "          buildInputs = [];",
      "        };",
      "      }",
      "    );",
      "}"
    ]
  }
}

